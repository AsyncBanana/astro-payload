{
	"name": "astro-payload",
	"version": "0.0.1",
	"description": "A simple way to add features to your Astro project",
	"main": "dist/index.js",
	"scripts": {
		"dev": "node scripts/dev.js && node dist/cli/index.js",
		"build": "node scripts/build.js"
	},
	"bin": "dist/cli/index.js",
	"exports": {
		".": "./dist/index.js",
		"./utils": "./dist/utils/index.js",
		"./recipes/*": "./dist/recipes"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AsyncBanana/astro-payload.git"
	},
	"keywords": [
		"Astro",
		"Scaffolding",
		"Plugins",
		"Template"
	],
	"author": "AsyncBanana",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/AsyncBanana/astro-payload/issues"
	},
	"homepage": "https://github.com/AsyncBanana/astro-payload#readme",
	"devDependencies": {
		"esbuild": "^0.13.3",
		"esbuild-plugin-d.ts": "^1.0.6",
		"esbuild-plugin-glob": "^1.0.3",
		"mico-spinner": "^1.3.0",
		"typescript": "^4.4.3"
	},
	"dependencies": {
		"commander": "^8.2.0",
		"enquirer": "^2.3.6",
		"execa": "^5.1.1",
		"picocolors": "^0.2.1"
	},
	"type": "module",
	"files": [
		"dist"
	]
}
